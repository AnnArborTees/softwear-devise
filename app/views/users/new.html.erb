<h1>Users <small>Create a new user</small></h1>

<div class='box-info row clear'>
  <%= bootstrap_form_for(@user, url: { action: 'create' }) do |f| %>

    <%= render partial: 'users/fields', locals: { f: f } %>

    <div class='form-group'>
    <%= f.submit 'Create' %>
    </div>
  <% end %>
</div>
